	"User" -> "Bath" [arrowtail=odot, arrowhead=crow, dir=both color="#90A377"]
	"User" -> "Review" [arrowtail=odot, arrowhead=crow, dir=both color="#D311B3"]
	"User" -> "Flag" [arrowtail=odot, arrowhead=crow, dir=both color="#A34034"]
	"User" -> "BathImage" [arrowtail=odot, arrowhead=crow, dir=both color="#72C5AE"]
	"Review" -> "Flag" [arrowtail=odot, arrowhead=crow, dir=both color="#62E372"]
	"Bath" -> "Review" [arrowtail=odot, arrowhead=crow, dir=both color="#A95AC0"]
	"Bath" -> "BathImage" [arrowtail=odot, arrowhead=crow, dir=both color="#31FE27"]
}
